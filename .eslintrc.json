{
	"plugins": ["jest", "react"],
	"env": {
		"jest/globals": true
	},
	"extends": ["next", "eslint:recommended", "plugin:react/recommended"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"react/react-in-jsx-scope": "off",
		"react/prop-types": "off"
	}
}
